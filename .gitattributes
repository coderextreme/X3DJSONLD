#
# https://help.github.com/articles/dealing-with-line-endings/
# Linux start script should use lf
/gradlew        text eol=lf
# These are Windows script files and should use crlf
*.bat           text eol=crlf
# Binary files should be left untouched
*.jar           binary
src/main/ppp/john3.json filter=lfs diff=lfs merge=lfs -text
src/main/ppp/john2.json filter=lfs diff=lfs merge=lfs -text
src/main/ppp/john.json filter=lfs diff=lfs merge=lfs -text
src/main/ppp/Gramps8Final.json filter=lfs diff=lfs merge=lfs -text
src/main/three2/data/john.three2.js filter=lfs diff=lfs merge=lfs -text
src/main/three2/data/Gramps8Final.three2.js filter=lfs diff=lfs merge=lfs -text
src/main/three2/data/john3.three2.js filter=lfs diff=lfs merge=lfs -text
src/main/three2/data/john2.three2.js filter=lfs diff=lfs merge=lfs -text
*.jar filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.nrrd filter=lfs diff=lfs merge=lfs -text
*.mpv filter=lfs diff=lfs merge=lfs -text
*.stl filter=lfs diff=lfs merge=lfs -text
*.hdr filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.so filter=lfs diff=lfs merge=lfs -text
*.glb filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
