							<xs:maxInclusive value="3.1416"/>
							<xs:attribute name="componentName" type="componentNameChoices" fixed="EnvironmentalEffects"/>
							<xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="2"/>
