				</xs:attribute><xs:element name="children" type="MFNode" fixed="outputOnlyField" default="X3DChildNode"/><xs:element name="geoOrigin" type="SFNode" fixed="initializeOnlyField" default="GeoOrigin"/><xs:element name="rootNode" type="MFNode" fixed="initializeOnlyField" default="X3DChildNode"/><xs:attribute name="additionalInterface" type="xs:string" default="X3DBoundedObject"/><xs:attribute name="componentName" type="componentNameChoices" fixed="Geospatial"/><xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="1"/></xs:appinfo>
