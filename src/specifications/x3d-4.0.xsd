							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="zSpacing" default="1.0">
						<!-- type="SFDoublePositive" -->
						<xs:simpleType>
							<xs:restriction base="SFDouble">
								<xs:minExclusive value="0"/>
							</xs:restriction>
