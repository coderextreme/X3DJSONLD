            </xs:appinfo>
			<xs:documentation source="https://www.web3d.org/specifications/X3Dv4/ISO-IEC19775-1v4-IS/Part01/components/core.html#X3DPrototypeInstance"/>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="fieldValue" minOccurs="0" maxOccurs="unbounded"/>
