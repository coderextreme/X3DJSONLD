					<xs:annotation>
						<xs:appinfo>
							<xs:attribute name="componentName" type="componentNameChoices" fixed="Time"/>
							<xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="2"/>
						</xs:appinfo>
					</xs:annotation>
