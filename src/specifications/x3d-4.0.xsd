						<xs:element ref="Layer"/>
						<xs:element ref="LayoutLayer"/>
						<xs:element ref="ProtoInstance">
							<xs:annotation>
								<xs:documentation>Appropriately typed substitution node</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:attribute name="activeLayer" type="SFInt32" default="0">
