						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="momentsOfInertia" type="MFFloat" default="0 0 0 0 0 0 0 0 0">
						<xs:annotation>
							<xs:appinfo>
								<xs:minInclusive value="0"/>
							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="containerField" type="containerFieldChoicesHAnimSegment" default="children"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
