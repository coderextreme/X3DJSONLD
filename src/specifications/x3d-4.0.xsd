						<xs:documentation>children</xs:documentation>
					</xs:annotation>
					<xs:group ref="ChildContentModel" minOccurs="0" maxOccurs="unbounded"/>
					<xs:group ref="ChildContentModelSceneGraphStructure" minOccurs="0" maxOccurs="unbounded"/>
