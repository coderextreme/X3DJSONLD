	<xs:complexType name="X3DChaserNode" abstract="true">
		<xs:annotation>
			<xs:appinfo><xs:attribute name="componentName" type="componentNameChoices" fixed="Followers"/><xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="1"/>
                The X3DChaserNode abstract node type calculates the output on value_changed as a finite impulse response (FIR)
                based on the events received on set_destination field.
            </xs:appinfo>
			<xs:documentation source="https://www.web3d.org/specifications/X3Dv4/ISO-IEC19775-1v4-IS/Part01/components/followers.html#X3DChaserNode"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="X3DFollowerNode">
				<xs:attribute name="duration" default="1">
					<!-- type="SFTimeNonNegative" -->
					<xs:simpleType>
						<xs:restriction base="SFTime">
							<xs:minInclusive value="0"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="X3DDamperNode" abstract="true">
		<xs:annotation>
			<xs:appinfo><xs:attribute name="componentName" type="componentNameChoices" fixed="Followers"/><xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="1"/>
                The X3DDamperNode abstract node type creates an IIR response that approaches the destination value
                according to the shape of the e-function only asymptotically but very quickly. 
            </xs:appinfo>
			<xs:documentation source="https://www.web3d.org/specifications/X3Dv4/ISO-IEC19775-1v4-IS/Part01/components/followers.html#X3DDamperNode"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="X3DFollowerNode">
				<xs:attribute name="tau" default="0.3">
					<!-- type="SFTimeNonNegative" -->
					<xs:simpleType>
						<xs:restriction base="SFTime">
							<xs:minInclusive value="0"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="tolerance" type="SFFloat" default="-1"/>
				<xs:attribute name="order" default="3">
					<xs:simpleType>
						<xs:restriction base="SFInt32">
							<xs:minInclusive value="0"/>
							<xs:maxInclusive value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
