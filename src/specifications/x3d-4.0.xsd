				<xs:annotation>
					<xs:appinfo><xs:documentation source="https://www.web3d.org/specifications/X3Dv4/ISO-IEC19775-1v4-IS/Part01/glossary.html#BindableNode"/><xs:attribute name="alias" type="xs:string" default="bindable node"/>
