							</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="order" type="MFInt32" default="0">
						<xs:annotation>
							<xs:appinfo>
								<xs:minInclusive value="0"/>
							</xs:appinfo>
						</xs:annotation>
