			<xs:documentation source="https://www.web3d.org/specifications/X3Dv4/ISO-IEC19775-1v4-IS/Part01/components/shaders.html#X3DVertexAttributeNode"/>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="X3DGeometricPropertyNode">
				<xs:attribute name="name" type="SFString">
