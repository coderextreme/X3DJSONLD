			<xs:appinfo><!-- global attribute default value can differ, and so is defined individually for each light node. --><!-- <xs:attribute name="global" type="SFBool" fixed="inputOutputField"/> --><xs:attribute name="componentName" type="componentNameChoices" fixed="Lighting"/><xs:attribute name="componentLevel" type="xs:positiveInteger" fixed="1"/>
