				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CAMERASPACENORMAL">
				<xs:annotation>
					<xs:appinfo>Use vertex normal, transformed to camera space, as input texture coordinates</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CAMERASPACEPOSITION">
