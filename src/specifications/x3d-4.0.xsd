                    </xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="event">
				<xs:annotation>
					<xs:appinfo><xs:documentation source="https://www.web3d.org/specifications/X3Dv4/ISO-IEC19775-1v4-IS/Part01/glossary.html#Event"/>
