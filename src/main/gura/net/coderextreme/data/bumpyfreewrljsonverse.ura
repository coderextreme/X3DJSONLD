(.setSourceCode "\\newlinefunction initialize() {\\newline     var resolution = 100;\\newline     var theta = 0.0;\\newline     var phi = 0.0;\\newline     var delta = (2 * 3.141592653) / (resolution-1);\\newline     var crds = new MFVec3f();\\newline     var vecCount = 0;\\newline     for ( i = 0; i < resolution; i++) {\\newline     	for ( j = 0; j < resolution; j++) {\\newline		var rho = a + b * Math.cos(c * theta + tdelta) * Math.cos(d * phi + pdelta);\\newline		crds[vecCount] = new SFVec3f(\\newline			rho * Math.cos(phi) * Math.cos(theta),\\newline			rho * Math.cos(phi) * Math.sin(theta),\\newline			rho * Math.sin(phi)\\newline		);\\newline		vecCount++;\\newline		theta += delta;\\newline	}\\newline	phi += delta;\\newline     }\\newline     coordinates = crds;\\newline     // coordinates = new MFVec3f(...crds);\\newline\\newline\\newline     var cis = new MFInt32();\\newline     var intCount = 0;\\newline     for ( i = 0; i < resolution-1; i++) {\\newline     	for ( j = 0; j < resolution-1; j++) {\\newline	     cis[intCount++] = i*resolution+j;\\newline	     cis[intCount++] = i*resolution+j+1;\\newline	     cis[intCount++] = (i+1)*resolution+j+1;\\newline	     cis[intCount++] = (i+1)*resolution+j;\\newline	     cis[intCount++] = -1;\\newline	}\\newline    }\\newline    coordIndexes = cis;\\newline    // coordIndexes = new MFInt32(...cis);\\newline}\\newline")                children: ROUTE
                    fromField: "coordIndexes"
                    fromNode: "OrbitScript"
                    toField: "set_coordIndex"
                    toNode: "Orbit"
                children: ROUTE
                    fromField: "coordinates"
                    fromNode: "OrbitScript"
                    toField: "set_point"
                    toNode: "OrbitCoordinates"
