(.setSourceCode "\\newlineconst newTranslation = function(Value) {\\newline	'use strict';\\newline	protoValue_changed = Value.x * protoScale;\\newline	protoText_changed = new MFString(protoParameterName+'='+protoValue_changed.toFixed(2));\\newline\\newline        var orientation = Value.x;\\newline\\newline        var ps = Browser.currentScene.getNamedNode(\"protoSensor\");\\newline        var t = Browser.currentScene.getNamedNode(\"protoTransform\");\\newline        var txt = Browser.currentScene.getNamedNode(\"protoText\");\\newline        if (shader) {\\newline            shader.getField(protoParameterName).setValue(orientation * protoScale);\\newline        }\\newline        if (txt) {\\newline            var stringField = txt.getField(\"string\");\\newline            var label = protoParameterName;\\newline            stringField.setValue(new MFString(label+\"=\"+(orientation * protoScale).toFixed(2)));\\newline        }\\newline        if (ps) {\\newline            ps.offset = new SFVec3f(orientation, ps.offset[1], ps.offset[2]);\\newline        }\\newline        if (t) {\\newline            t.translation = new SFVec3f(orientation, t.translation[1], t.translation[2]);\\newline        }\\newline}\\newline")                            children: ROUTE
                                fromField: "translation_changed"
                                fromNode: "protoSensor"
                                toField: "set_translation"
                                toNode: "protoTransform"
                            children: ROUTE
                                fromField: "translation_changed"
                                fromNode: "protoSensor"
                                toField: "newTranslation"
                                toNode: "protoValueTransformerScript"
                            children: ROUTE
                                fromField: "protoText_changed"
                                fromNode: "protoValueTransformerScript"
                                toField: "string"
