                    ],
                    ROUTE: [
                        fromField: "addedEntities"
                        fromNode: "EntityManager"
                        toField: "addChildren"
                        toNode: "AvatarHolder"
                    ],
                    ROUTE: [
                        fromField: "removedEntities"
