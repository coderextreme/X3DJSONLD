                                field: [
                                    name: "sensorTranslation"
                                    accessType: "inputOutput"
                                    type: "SFVec3f"
                                    value: "0 0 0"
                                ],
                                field: [
                                    name: "numberTranslation"
                                    accessType: "inputOutput"
                                    type: "SFVec3f"
                                    value: "0.2 0 0"
                                ],
                                field: [
                                    name: "textString"
                                    accessType: "inputOutput"
                                    type: "MFString"
                                    value: "\"a=\""
