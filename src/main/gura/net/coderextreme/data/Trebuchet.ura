#sourceCode "\\newline\\newlinefunction initialize ()\\newline{\\newline	MassCounterWeight=100;\\newline	MassProjectileWeight=10;\\newline	Browser.println ('MassCounterWeight =' + MassCounterWeight);\\newline	Browser.println ('MassProjectileWeight =' + MassProjectileWeight);\\newline}\\newline\\newlinefunction set_MassProjectileWeight (value, timestamp)\\newline{\\newline	MassProjectileWeight = value;\\newline	Browser.println ('new MassProjectileWeight =' + MassProjectileWeight);\\newline}\\newlinefunction set_MassCounterWeight (value2, timestamp)\\newline{\\newline	MassCounterWeight = value2;\\newline	Browser.println ('new MassCounterWeight =' + MassCounterWeight);\\newline}\\newline\\newline\\newlinefunction set_fraction ( fraction, eventTime )\\newline{\\newline	var TrebuchetHeight=45;\\newline	var Height =25;\\newline      	x = (2*(MassCounterWeight/MassProjectileWeight)*Height*fraction)-.5*TrebuchetHeight;\\newline	// start at TrebuchetHeight and keep along z axis (z=zero)\\newline	y =  (MassCounterWeight/MassProjectileWeight)*Height*Math.sin(fraction*3.14);\\newline        z = 0;\\newline        transparent = new SFVec3f (1,1,1);\\newline     if (y <1.5*TrebuchetHeight  )if (x<TrebuchetHeight)\\newline		{\\newline		x=x-10;\\newline		y=y+1;\\newline		transparent = new SFVec3f(.01, .01, .01);\\newline			}\\newline\\newline	value_changed = new SFVec3f (x, y, z);\\newline	Browser.println ('x=' + value_changed[0] + ' y=' + value_changed[1] + ' z=' + value_changed[2]);\\newline}\\newline"
