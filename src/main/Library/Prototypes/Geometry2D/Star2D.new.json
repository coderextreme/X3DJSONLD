"ecmascript:",
"",
"var yAxis = new SFVec3f (0, 1, 0);",
"",
"function initialize ()",
"{",
"\teventsProcessed ();",
"}",
"",
"function eventsProcessed ()",
"{",
"\tvar",
"\t\tcoordIndex = new MFInt32 (),",
"\t\tcoord      = geometry .coord,",
"\t\ttexCoord   = geometry .texCoord;",
"",
"\tcoord    .point .length = 0;",
"\ttexCoord .point .length = 0;",
"\t",
"\tvar",
"\t\tmaxRadius = Math .max (Math .abs (innerRadius), Math .abs (outerRadius)),",
"\t\ttexOffset = new SFVec3f (maxRadius, maxRadius, 0);",
"",
"\tfor (var i = 0, length = sides * 2; i < length; ++ i)",
"\t{",
"\t\tvar",
"\t\t\trotation = new SFRotation (0, 0, 1, Math .PI * 2 * i / length),",
"\t\t\tradius   = Math .abs (i % 2 ? innerRadius : outerRadius),",
"\t\t\tpoint    = rotation .multVec (yAxis) .multiply (radius),",
"\t\t\ttex      = point .add (texOffset) .divide (maxRadius * 2);",
"",
"\t\tcoordIndex .push (i);",
"\t\tcoord .point .push (point);",
"\t\ttexCoord .point .push (new SFVec2f (tex .x, tex .y));",
"\t}",
"\t",
"\tcoordIndex .push (-1);",
"",
"\tif (! solid)",
"\t{",
"\t\tfor (var i = 0, length = sides * 2; i < length; ++ i)",
"\t\t\tcoordIndex .push (length - 1 - i);",
"",
"\t\tcoordIndex .push (-1);",
"\t}",
"",
"\tgeometry .set_coordIndex = coordIndex;",
"}",
