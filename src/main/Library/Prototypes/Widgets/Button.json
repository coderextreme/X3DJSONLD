"",
"ecmascript:",
"var DEFAULT  = 0;",
"var OVER     = 1;",
"var ACTIVE   = 2;",
"var DISABLED = 3;",
"",
"function initialize ()",
"{",
"\tchoose ();",
"}",
"",
"function set_enabled (value)",
"{",
"\tset_whichChoice (value ? get (DEFAULT) : get (DISABLED));",
"}",
"",
"function set_active ()",
"{",
"\tchoose ();",
"}",
"",
"function set_over ()",
"{",
"\tchoose ();",
"}",
"",
"function set_whichChoice (value)",
"{",
"\tif (value < button .children .length)",
"\t\tbutton .whichChoice = value;",
"",
"\telse",
"\t\tbutton .whichChoice = get (DEFAULT);",
"}",
"",
"function choose ()",
"{",
"\tif (touchSensor .isActive)",
"\t\tset_whichChoice (touchSensor .isOver ? get (ACTIVE) : get (DEFAULT));",
"",
"\telse if (touchSensor .isOver)",
"\t\tset_whichChoice (get (OVER));",
"",
"\telse",
"\t\tset_whichChoice (get (DEFAULT));",
"}",
"",
"function set_children ()",
"{",
"\tchoose ();",
"}",
"",
"function get (value)",
"{",
"\tif (value < button .children .length)",
"\t\treturn value;",
"",
"\tif (DEFAULT < button .children .length)",
"\t\treturn DEFAULT;",
"",
"\treturn -1;",
"}",
"",
"",
""
