"",
"ecmascript:",
"",
"function initialize ()",
"{",
"\tset_children (button .children);",
"\tinitialized = 0;",
"}",
"",
"function set_initialized (value, time)",
"{",
"\tset_enabled (enabled, time);",
"}",
"",
"function set_enabled (value, time)",
"{",
"\tif (value)",
"\t{",
"\t\tisActive  = TRUE;",
"\t\tenterTime = time;\t",
"\t}",
"\telse",
"\t{",
"\t\tisActive = FALSE;",
"\t\texitTime = time;",
"\t}",
"",
"\tchoose ();",
"} ",
"",
"function set_disabled (value, time)",
"{",
"\tenabled = false;",
"}",
"",
"function set_active (value, time)",
"{",
"\tenabled = true;",
"}",
"",
"function set_children (value)",
"{",
"\tif (children .length > 1)",
"\t{",
"\t\tBrowser .deleteRoute (children [0], 'touchTime', self, 'set_active');",
"\t\tBrowser .deleteRoute (children [1], 'touchTime', self, 'set_disabled');",
"\t}",
"",
"\tchildren = value;",
"",
"\tif (children .length > 1)",
"\t{",
"\t\tBrowser .addRoute (children [0], 'touchTime', self, 'set_active');",
"\t\tBrowser .addRoute (children [1], 'touchTime', self, 'set_disabled');",
"\t}",
"}",
"",
"function choose ()",
"{",
"\tbutton .whichChoice = isActive;",
"}",
"",
""
