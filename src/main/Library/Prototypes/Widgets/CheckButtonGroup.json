"ecmascript:",
"",
"function initialize ()",
"{",
"\tset_children (buttons .children);",
"}",
"",
"function set_whichChoice (value)",
"{",
"\tfor (var i = 0; i < buttons .children .length; ++ i)",
"\t{",
"\t\tif (buttons .children [i] .enabled != whichChoice [i])",
"\t\t\tbuttons .children [i] .enabled = whichChoice [i];",
"\t}",
"}",
"",
"function set_active ()",
"{",
"\twhichChoice .length = 0;",
"",
"\tfor (var i = 0; i < buttons .children .length; ++ i)",
"\t{",
"\t\twhichChoice [i] = buttons .children [i] .isActive;",
"\t}",
"}",
"",
"function set_children (value)",
"{",
"\tfor (var i = 0; i < children .length; ++ i)",
"\t\tBrowser .deleteRoute (children [i], 'isActive', self, 'set_active');",
"\t\t",
"\tchildren = value;",
"",
"\tfor (var i = 0; i < children .length; ++ i)",
"\t\tBrowser .addRoute (children [i], 'isActive', self, 'set_active');",
"\t\t",
"\tset_active ();",
"}",
