"",
"ecmascript:",
"",
"function initialize ()",
"{",
"\tset_children (buttons .children);",
"}",
"",
"function set_whichChoice (value)",
"{",
"\tif (whichChoice > -1 && whichChoice < buttons .children .length)",
"\t{",
"\t\tvar checkButton = buttons .children [whichChoice];",
"\t\tcheckButton .enabled = FALSE;",
"\t\tcheckButton .children [1] .enabled = TRUE;",
"\t}",
"\t",
"\twhichChoice = value;",
"",
"\tif (whichChoice > -1 && whichChoice < buttons .children .length)",
"\t{",
"\t\tvar checkButton = buttons .children [whichChoice];",
"\t\tcheckButton .enabled = TRUE;",
"\t\tcheckButton .children [1] .enabled = FALSE;",
"\t}",
"\telse",
"\t{",
"\t\t// Disable all buttons",
"",
"\t\tfor each (var checkButton in buttons .children)",
"\t\t{",
"\t\t\tcheckButton .enabled = FALSE;",
"\t\t\tcheckButton .children [1] .enabled = TRUE;",
"\t\t}",
"\t}",
"}",
"",
"function set_touchTime (value)",
"{",
"\tfor (var i = 0; i < buttons .children .length; ++ i)",
"\t{",
"\t\tif (value == buttons .children [i] .enterTime)",
"\t\t{",
"\t\t\tif (i != whichChoice)",
"\t\t\t\tfields .whichChoice = i;",
"",
"\t\t\tbreak;",
"\t\t}",
"\t}",
"}",
"",
"function set_children (value)",
"{",
"\tfor (var i = 0; i < children .length; ++ i)",
"\t\tBrowser .deleteRoute (children [i], 'enterTime', self, 'set_touchTime');",
"\t\t",
"\tchildren = value;",
"",
"\tfor (var i = 0; i < children .length; ++ i)",
"\t\tBrowser .addRoute (children [i], 'enterTime', self, 'set_touchTime');",
"\t\t",
"\tset_whichChoice (fields .whichChoice);",
"}",
"",
""
