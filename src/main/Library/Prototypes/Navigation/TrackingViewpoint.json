"",
"function get_orientation (fromPoint, toPoint)",
"{",
"\tvar direction = fromPoint .subtract (toPoint);",
"\tvar rotation  = new SFRotation (zAxis, direction);",
"\tvar cameraUp  = rotation .multVec (yAxis);",
"\tvar N1        = direction .cross (cameraUp);",
"\tvar N2        = direction .cross (yAxis);",
"\tvar straight  = new SFRotation (N1, N2);",
"",
"\treturn rotation .multiply (straight);",
"}",
