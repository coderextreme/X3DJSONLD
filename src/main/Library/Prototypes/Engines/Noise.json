"",
"ecmascript:",
"",
"// PEACHEYnoise",
"",
"var xknots = [ ];",
"var yknots = [ ];",
"var zknots = [ ];",
"",
"function initialize ()",
"{",
"\tfor (i = 0; i < TABSIZE; i++)",
"\t\tVALUTAB [i] = 1.0 - 2.0 * Math .random ();",
"}",
"",
"function set_time (time)",
"{",
"\tvalue_changed = amplitude * vnoise (time * frequency) + phase;",
"}",
"",
"function vnoise (value)",
"{",
"\txknots .length = 0;",
"\tyknots .length = 0;",
"\tzknots .length = 0;",
"",
"\tvar ix = Math .floor (value);",
"\tvar fx = value - ix;",
"",
"\tfor (k = -1; k <= 2; k ++)",
"\t{",
"\t\tfor (j = -1; j <= 2; j ++)",
"\t\t{",
"\t\t\tfor (i = -1; i <= 2; i ++)",
"\t\t\t\txknots [i + 1] = vlattice (ix + i, ix + j, k);",
"",
"\t\t\tyknots [j + 1] = spline (fx, 4, xknots);",
"\t\t}",
"",
"\t\tzknots [k + 1] = spline (fx, 4, yknots);",
"\t}",
"",
"\treturn spline (0, 4, zknots);",
"}",
"",
"function spline (x, nknots, knot)",
"{",
"\tvar i      = 0;",
"\tvar nspans = nknots - 3;",
"",
"\tif (nspans >= 1)",
"\t{",
"\t\t/* Find the appropriate 4-point span of the spline. */",
"\t\tvar x    = clamp(x, 0, 1) * nspans;",
"\t\tvar span = Math.floor(x);",
"\t\t",
"\t\tif (span >= (nknots - 3))",
"\t\t\tspan = (nknots - 3);",
"\t\t",
"\t\tx -= span;",
"\t\ti += span;",
"",
"\t\t/* Evaluate the span cubic at x using Horners rule. */",
"\t\tvar c3 = CR00 * knot [0 + i] + CR01 * knot [1 + i] + CR02 * knot [2 + i] + CR03 * knot [3 + i];",
"\t\tvar c2 = CR10 * knot [0 + i] + CR11 * knot [1 + i] + CR12 * knot [2 + i] + CR13 * knot [3 + i];",
"\t\tvar c1 = CR20 * knot [0 + i] + CR21 * knot [1 + i] + CR22 * knot [2 + i] + CR23 * knot [3 + i];",
"\t\tvar c0 = CR30 * knot [0 + i] + CR31 * knot [1 + i] + CR32 * knot [2 + i] + CR33 * knot [3 + i];",
"",
"\t\treturn ((c3 * x + c2) * x + c1) * x + c0;",
"\t}",
"}",
"",
"function perm (x)",
"{",
"\treturn PERM [(x & TABMASK)];",
"}",
"",
"function index (ix, iy, iz)",
"{",
"\treturn perm (ix + perm (iy + perm (iz)));",
"}",
"",
"function vlattice (ix, iy, iz)",
"{",
"\treturn VALUTAB [index (ix, iy, iz)];",
"}",
"",
"function clamp (x, a, b)",
"{",
"\treturn Math .min (Math .max (x, a), b);",
"}",
"",
""
