"vrmlscript:",
"function set_time(value, time)",
"{",
"\tif (time - last > 1 / fps){",
"\t\tlast = time;",
"\t\tif(++count > maxChoice) count = 0;",
"\t\twhichChoice_changed = count;",
"\t}",
"}",
"",
"function isActive(value, time)",
"{",
"\twhichChoice_changed = -1;",
"}",
