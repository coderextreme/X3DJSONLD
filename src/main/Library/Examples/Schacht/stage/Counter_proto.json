"vrmlscript:",
"function set_offset(value, time)",
"{",
"\tif (value < 0) {",
"\t\toffset = 0;",
"\t} else if (value > data.value) {",
"\t\toffset = data.value;",
"\t} else {",
"\t\toffset = value;",
"\t}",
"}",
"function set_startTime(value, time)",
"{",
"\tif (!isActive) {",
"\t\tisActive = TRUE;",
"\t\tenterTime = time;",
"\t\toffset = 0;",
"\t} else {",
"\t\t++offset;",
"\t\tif (offset >= data.value) {",
"\t\t\tisActive = FALSE;",
"\t\t\texitTime = time;",
"\t\t}",
"\t}",
"\twhichChoice_changed = offset;",
"\tfraction_changed = offset / data.value;",
"}",
"function set_stopTime(value, time) {",
"\tif (isActive) {",
"\t\tisActive = FALSE;",
"\t\texitTime = time;",
"\t}",
"}",
"function initialize() { if (data.startTime) data.startTime = 1; }",
