"vrmlscript:",
"function set_offset(value, time)",
"{",
"\tif (offset == value) { return; }",
"",
"\tif (data.enabled) {",
"\t\tnext = TRUE;",
"\t} else {",
"\t\tnext = FALSE;",
"\t\toffset = value;",
"\t\tif (value) {",
"\t\t\tif (!isActive) isActive = value;",
"\t\t\tenterTime = time;\t\t\t",
"\t\t} else {",
"\t\t\tif (isActive) {",
"\t\t\t\texitTime = time;",
"\t\t\t}",
"\t\t}",
"\t}",
"}",
"function set_enabled(value, time)",
"{",
"\tif (!value && next) {",
"\t\tif (offset != data.offset) data.offset = !offset;",
"\t} else if (!value && !offset && !next) {",
"\t\tisActive = FALSE;",
"\t}",
"}",
"function set_startTime(value, time) { data.offset = TRUE; }",
"function set_stopTime(value, time) { data.offset = FALSE; }",
