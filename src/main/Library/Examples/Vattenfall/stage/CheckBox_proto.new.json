"vrmlscript:",
"function set_touchTime (value, time) {",
"\tif (data.enabled) button.whichChoice = !isActive;",
"}",
"function set_on (value, time) {",
"\tif (data.enabled && value != button.whichChoice) button.whichChoice = value;",
"}",
"function set_whichChoice (value, time) {",
"\tif (data.enabled) isActive = value;",
"}",
"function initialize() {\t",
