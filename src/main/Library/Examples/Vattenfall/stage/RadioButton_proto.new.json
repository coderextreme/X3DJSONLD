"vrmlscript:",
"function set_touchTime (value, time) {",
"\tif (!data.enabled) return;",
"\tfor (i=0; i<children.length; ++i) {",
"\t\tif (children[i].touchTime == value) {",
"\t\t\ttouchTime = time;",
"\t\t\tdata.whichChoice = i;",
"\t\t\tbreak;",
"\t\t}",
"\t}",
"}",
"function set_whichChoice (value, time) {",
"\tif (whichChoice >= 0 && whichChoice < children.length)",
"\t\tchildren[whichChoice].on = FALSE;",
"\twhichChoice = value;",
"\tif (touchTime != time)",
"\t\tif (whichChoice >= 0 && whichChoice < children.length && !children[whichChoice].whichChoice)",
"\t\t\tchildren[whichChoice].on = TRUE;",
"}",
"function set_children (value, time) {",
"\tfor (i=0; i<children.length; ++i)",
"\t\tBrowser.deleteRoute(children[i], 'touchTime', self, 'set_touchTime');",
"\tchildren = value;",
"\tfor (i=0; i<children.length; ++i) {",
"\t\tchildren[i].on = FALSE;",
"\t\tBrowser.addRoute(children[i], 'touchTime', self, 'set_touchTime');",
"\t}",
"\tif (whichChoice >= 0 && whichChoice < children.length)",
"\t\tchildren[whichChoice].on = TRUE;",
"}",
"function initialize() {",
"\twhichChoice = data.whichChoice;",
"\tset_children(data.children, 0);",
"}",
