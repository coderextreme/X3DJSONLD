"vrmlscript:",
"function query() {",
"\tq = '';",
"\tfor (i = 0; i < data.parameter.length; i++) {",
"\t\tq += ';' + data.parameter[i].name;",
"\t\tif (data.parameter[i].value.length) {",
"\t\t\tq += '=' + data.parameter[i].value[0];",
"\t\t}",
"\t}",
"\tif (q.length > 1) return q.substring(1, q.length);",
"\treturn '';",
"}",
"function url() {",
"\tq = query();",
"\tu = new MFString();",
"\tu.length = data.url.length;",
"",
"\tif (q == '') return data.url;",
"\tfor (i = 0; i < u.length; i++) {",
"\t\tu[i] = data.url[i] + (data.url[i].indexOf('?') > -1 ? (data.url[i].indexOf('?') == data.url[i].length-1  ? '' : (q.length ? (data.url[i].indexOf(';') == data.url[i].length-1  ? '' : ';') : '') ) : '?') + q;",
"\t}",
"\treturn u;",
"}",
"function loadUrl() {",
"\tif(data.url.length && !isActive) {",
"\t\tisActive = TRUE;",
"\t\tBrowser.createVrmlFromURL(url(), self, \"set_children\");",
"\t}",
"}",
"function set_url(value, time) { loadUrl(); }",
"function set_startTime(value, time) { loadUrl(); }",
"function set_children(value, time) {",
"\tif(isActive){",
"\t\tif (value.toString() == \"Group { }\") {",
"\t\t\tdata.children = value[0].children;",
"\t\t} else {",
"\t\t\tdata.children = value;",
"\t\t}",
"\t\tisActive = FALSE;",
"\t\texitTime = time;",
"\t}",
"}",
"function initialize() { if (data.startTime) loadUrl(); }",
"",
