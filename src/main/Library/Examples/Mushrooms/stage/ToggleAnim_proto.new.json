"",
"function initialize()",
"{",
"\tforwardTimer.enabled = FALSE;",
"\tbackTimer.enabled = FALSE;",
"",
"\tif (data.offset){",
"\t\tbackTimer.fraction_changed = 0;",
"\t}else {",
"\t\tforwardTimer.fraction_changed = 0;",
"\t}",
"\ttoggled = data.offset;",
"}",
"",
"function startTime(value, time)",
"{",
"\tif (!toggled){",
"\t\tif(animating) click = TRUE;",
"\t\telse{",
"\t\t\tforwardTimer.enabled   = TRUE;",
"\t\t\tforwardTimer.startTime = time;",
"\t\t}",
"\t}else {",
"\t\tif(animating) click = TRUE;",
"\t\telse{",
"\t\t\tbackTimer.enabled   = TRUE;",
"\t\t\tbackTimer.startTime = time;",
"\t\t}",
"\t}\t",
"}",
"",
"function set_animating(value, time)",
"{",
"\tanimating = value;",
"\tif (value){",
"\t\tclick = FALSE;",
"\t} else {",
"\t\ttoggled = !toggled;",
"\t\tif (click){",
"\t\t\tif (toggled){",
"\t\t\t\tbackTimer.enabled   = TRUE;",
"\t\t\t\tbackTimer.startTime = time;",
"\t\t\t}else {",
"\t\t\t\tforwardTimer.enabled   = TRUE;",
"\t\t\t\tforwardTimer.startTime = time;",
"\t\t\t}\t",
"\t\t}",
"\t}",
"}",
"",
"function set_offset(value, time)",
"{",
"\tclick = FALSE;",
"\tforwardTimer.enabled = FALSE;",
"\tbackTimer.enabled = FALSE;",
"",
"\tif (value){",
"\t\tbackTimer.fraction_changed = 0;",
"\t}else {",
"\t\tforwardTimer.fraction_changed = 0;",
"\t}",
"\ttoggled = value;",
"}",
