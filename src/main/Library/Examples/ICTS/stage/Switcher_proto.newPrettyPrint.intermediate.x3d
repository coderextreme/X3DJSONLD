    <ProtoDeclare name='Switcher'>
      <ProtoInterface>
        <field name='minValue' accessType='inputOutput' type='SFInt32'/>
        <field name='maxValue' accessType='inputOutput' type='SFInt32' value='1'/>
        <field name='prev' accessType='inputOutput' type='SFTime'/>
        <field name='next' accessType='inputOutput' type='SFTime'/>
        <field name='offset' accessType='inputOutput' type='SFInt32'/>
        <field name='whichChoice_changed' accessType='outputOnly' type='SFInt32'/>
      </ProtoInterface>
      <ProtoBody>
        <ProtoDeclare name='Data'>
          <ProtoInterface>
            <field name='minValue' accessType='inputOutput' type='SFInt32'/>
            <field name='maxValue' accessType='inputOutput' type='SFInt32' value='1'/>
            <field name='prev' accessType='inputOutput' type='SFTime'/>
            <field name='next' accessType='inputOutput' type='SFTime'/>
            <field name='offset' accessType='inputOutput' type='SFInt32'/>
          </ProtoInterface>
          <ProtoBody>
            <Group/>
          </ProtoBody>
        </ProtoDeclare>
        <Group>
          <Script DEF='_switcher' directOutput='true'>
            <field name='set_minValue' accessType='inputOnly' type='SFInt32'/>
            <field name='set_maxValue' accessType='inputOnly' type='SFInt32'/>
            <field name='set_prev' accessType='inputOnly' type='SFTime'/>
            <field name='set_next' accessType='inputOnly' type='SFTime'/>
            <field name='whichChoice_changed' accessType='outputOnly' type='SFInt32'/>
            <field name='data' accessType='initializeOnly' type='SFNode'>
              <ProtoInstance DEF='_1' name='Data'>
                <IS>
                  <connect nodeField='minValue' protoField='minValue'/>
                  <connect nodeField='maxValue' protoField='maxValue'/>
                  <connect nodeField='prev' protoField='prev'/>
                  <connect nodeField='next' protoField='next'/>
                  <connect nodeField='offset' protoField='offset'/>
                </IS>
              </ProtoInstance>
            </field>
            <IS>
              <connect nodeField='whichChoice_changed' protoField='whichChoice_changed'/>
            </IS>
<![CDATA[
vrmlscript:
function  initialize()
{
	if (data.next == 1 || data.prev == 1) whichChoice_changed = data.offset;
}

function  set_prev(value, time)
{
	if (--data.offset < data.minValue) data.offset = data.maxValue;
	whichChoice_changed = data.offset;
}

function  set_next(value, time)
{
	if (++data.offset > data.maxValue) data.offset = data.minValue;
	whichChoice_changed = data.offset;
}

function set_minValue(value, time)
{
	if (data.offset < value) data.offset = value;
}

function set_maxValue(value, time)
{
	if (data.offset > value) data.offset = value;
}
]]>
          </Script>
