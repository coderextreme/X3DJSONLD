        <ROUTE fromField='prev_changed' fromNode='_1' toField='set_prev' toNode='_switcher'/>
        <ROUTE fromField='next_changed' fromNode='_1' toField='set_next' toNode='_switcher'/>
        <ROUTE fromField='minValue_changed' fromNode='_1' toField='set_minValue' toNode='_switcher'/>
        <ROUTE fromField='maxValue_changed' fromNode='_1' toField='set_maxValue' toNode='_switcher'/>
