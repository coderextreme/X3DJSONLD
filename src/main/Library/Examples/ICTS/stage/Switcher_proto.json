"function  initialize()",
"{",
"\tif (data.next == 1 || data.prev == 1) whichChoice_changed = data.offset;",
"}",
"",
"function  set_prev(value, time)",
"{",
"\tif (--data.offset < data.minValue) data.offset = data.maxValue;",
"\twhichChoice_changed = data.offset;",
"}",
"",
"function  set_next(value, time)",
"{",
"\tif (++data.offset > data.maxValue) data.offset = data.minValue;",
"\twhichChoice_changed = data.offset;",
"}",
"",
"function set_minValue(value, time)",
"{",
"\tif (data.offset < value) data.offset = value;",
"}",
"",
"function set_maxValue(value, time)",
"{",
"\tif (data.offset > value) data.offset = value;",
"}",
