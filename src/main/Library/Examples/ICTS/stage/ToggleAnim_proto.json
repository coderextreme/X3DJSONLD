"vrmlscript:",
"function initialize()",
"{",
"\tforwardTimer.stopTime = 0;",
"\tbackTimer.stopTime    = 0;",
"\tdirection = data.direction;",
"\tstate_changed = direction;",
"}",
"",
"function set_direction(value, time)",
"{",
"\tif (value == direction) return;",
"\tdirection = value;",
"\tif (direction == state_changed) {",
"\t\tnext = TRUE;",
"\t} else {",
"\t\tnext = FALSE;",
"\t\tif (direction) {",
"\t\t\tforwardTimer.enabled   = TRUE;",
"\t\t\tforwardTimer.startTime = time;\t\t\t",
"\t\t} else {",
"\t\t\tbackTimer.enabled\t= TRUE;",
"\t\t\tbackTimer.startTime = time;",
"\t\t}",
"\t}",
"}",
"",
"function startTime(value, time)",
"{",
"\tif (!next) set_direction(!direction, time);",
"}",
"",
"function forward(value, time)",
"{",
"\tset_direction(TRUE, time);",
"}",
"",
"function back(value, time)",
"{",
"\t set_direction(FALSE, time);",
"}",
"",
"function set_isActive(value, time)",
"{",
"\tisActive = value;",
"\tif (!isActive) {",
"\t\tif (next) {",
"\t\t\tdirection = !direction;",
"\t\t\tstate_changed = direction;",
"\t\t\tset_direction(!direction, time);",
"\t\t} else {",
"\t\t\tstate_changed = direction;",
"\t\t\tforwardTimer.enabled = FALSE;",
"\t\t\tbackTimer.enabled\t = FALSE;\t\t\t",
"\t\t}",
"\t}",
"}",
