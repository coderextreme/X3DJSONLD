"vrmlscript:",
"function  set_time(value, time)",
"{",
"    t = Math.floor(value);",
"        f = (value - t);",
"   for (i = 0; i < fraction.length; i++) {",
"                f *= 10;",
"                whichChoice = Math.floor(f);",
"                fraction[i].whichChoice = whichChoice;",
"        f -= whichChoice;",
"    }",
"    if (l > 0) {",
"        if (t != sec && l) {",
"                sec = t;",
"                s = sec % 60; t =  sec / 60;",
"                e = s % 10;",
"                children[0].whichChoice = e;",
"                if (l > 1) {",
"                        z = s / 10;",
"                        children[1].whichChoice = z;",
"                        if (t != min && l > 2) {",
"                                min = t;",
"                                m = min % 60; t = min / 60;",
"                                e = m % 10;",
"                                children[2].whichChoice = e;",
"                                if (l > 2) {",
"                                        z = m / 10;",
"                                        children[3].whichChoice = z;",
"                                        if (t != hour && l > 4) {",
"                                                hour = t;",
"                                                h = hour % 24; t = hour / 24;",
"                                                e = h  % 10;",
"                                                children[4].whichChoice = e;",
"                                                if (l > 5) {",
"                                                        z = h  / 10;",
"                                                        children[5].whichChoice = z;",
"                                                }",
"                                        }",
"                                }",
"                        }",
"                }",
"        }",
"    }",
"}",
