  <Scene>
    <ProtoDeclare name='EnableTouch'>
      <ProtoInterface>
        <field accessType='inputOnly' type='SFTime' name='touchTime'/>
        <field accessType='inputOnly' type='SFBool' name='isActive'/>
        <field accessType='outputOnly' type='SFBool' name='enabled_changed'/>
      </ProtoInterface>
      <ProtoBody>
        <Script DEF='_enableTouch'>
          <field accessType='inputOnly' type='SFTime' name='touchTime'/>
          <field accessType='inputOnly' type='SFBool' name='isActive'/>
          <field accessType='outputOnly' type='SFBool' name='enabled_changed'/>
          <IS>
            <connect nodeField='touchTime' protoField='touchTime'/>
            <connect nodeField='isActive' protoField='isActive'/>
            <connect nodeField='enabled_changed' protoField='enabled_changed'/>
          </IS>
<![CDATA[vrmlscript:
function touchTime(value, time)
{
	enabled_changed = FALSE;
}

function isActive(value, time)
{
	if (!value) enabled_changed = TRUE;
}
]]> 
        </Script>
      </ProtoBody>
    </ProtoDeclare>
  </Scene>
