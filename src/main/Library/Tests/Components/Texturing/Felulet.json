"",
"ecmascript:",
"",
"function initialize ()",
"{",
"\tprint ('*** Initializing Felület ...');",
"",
"\tsample1 .length = size;",
"\tsample2 .length = size;",
"\t",
"\tfor (i = 0; i < size * size; ++ i)",
"\t{",
"\t\tsample1 [i] = 100;",
"\t\tsample2 [i] = 100;",
"\t}",
"",
"//\tfor (x = 1; x < size-2; ++ x)",
"//\t{",
"//\t\tfor (y = 1; y < size-2; ++ y)",
"//\t\t{",
"//\t\t\tk = Math .random ();   ",
"//\t\t\tif (k > 0.5)",
"//\t         set_sample1 (x, y);",
"//\t\t}",
"//\t}",
"",
"\timage_changed.x    = size;",
"\timage_changed.y    = size;",
"\timage_changed.comp = 1;",
"",
"\tprint ('*** Done initializing Felület ...');",
"}",
"",
"function set_time (value, time)",
"{",
"\t// Randomly set n new points",
"\t",
"\tfor (i = 0; i < size/2; ++ i)",
"\t\tset_sample1 (Math .floor (1 + Math .random () * (size - 3)), 1 + Math .floor (Math .random () * (size - 3)));   ",
"",
"\tfor (x = 1; x < size - 1; ++ x)",
"\t{",
"\t\tfor (y = 1; y < size - 1; ++ y)",
"\t\t{",
"\t\t\tpiece = 0;",
"",
"\t\t\txe = x-1;",
"\t\t\txu = x+1;",
"\t\t\tye = y-1;",
"\t\t\tyu = y+1;",
"\t\t",
"\t\t\tif (get_sample1 (xe, ye) > 100) { ++ piece; }",
"\t\t\tif (get_sample1 (xe, y)  > 100) { ++ piece; }",
"\t\t\tif (get_sample1 (xe, yu) > 100) { ++ piece; }",
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  ",
"\t\t\tif (get_sample1 (xu, ye) > 100) { ++ piece; }",
"\t\t\tif (get_sample1 (xu, y)  > 100) { ++ piece; }",
"\t\t\tif (get_sample1 (xu, yu) > 100) { ++ piece; }",
"\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  ",
"\t\t\tif (get_sample1 (x, ye)  > 100) { ++ piece; }",
"\t\t\tif (get_sample1 (x, yu)  > 100) { ++ piece; }",
"\t",
"\t\t\t// counting new cells",
"\t\t",
"\t\t\tif (get_sample1 (x, y) == 100) ",
"\t\t\t{",
"\t\t\t\tif (piece == 3)",
"\t\t\t\t\tset_sample2 (x, y); ",
"\t\t\t\telse",
"\t\t\t\t\tclear_sample2 (x, y); ",
"\t\t\t}",
"\t",
"\t\t\tif (get_sample1 (x, y) > 100) ",
"\t\t\t{",
"\t\t\t \tif (piece < 2 || piece > 3)",
"\t\t\t\t\tclear_sample2 (x, y);",
"\t\t\t\telse",
"\t\t\t\t\tset_sample2 (x, y);",
"\t\t\t}",
"\t\t}",
"\t}",
"",
"\timage_changed .array = sample2;",
"",
"\tsample1 = sample2;",
"}",
"",
"function get_sample1 (x, y) { return sample1 [x + y * size]; }",
"",
"function set_sample1 (x, y) { sample1 [x + y * size] = 255; }",
"",
"function set_sample2 (x, y) { sample2 [x + y * size] = 255; }",
"",
"function clear_sample2 (x, y) { sample2 [x + y * size] = 100; }",
"",
""
