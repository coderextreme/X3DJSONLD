"uniform int x3d_GeometryType;",
"uniform x3d_MaterialParameters x3d_FrontMaterial;",
"",
"varying vec3 normal;",
"",
"void",
"main ()",
"{",
"\tfloat f = max (dot (normal, vec3 (0.0, 0.0, 1.0)), 0.0);",
"",
"\tvec3  color = x3d_FrontMaterial .diffuseColor * f + x3d_FrontMaterial .emissiveColor;",
"\tfloat alpha = 1.0 - x3d_FrontMaterial .transparency;",
"",
"\tgl_FragColor = vec4 (color, alpha);",
"}",
