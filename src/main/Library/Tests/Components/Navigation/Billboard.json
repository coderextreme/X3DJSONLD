"",
"vrmlscript:",
"function initialize ()",
"{",
"\tset_xyz ();",
"}",
"",
"function set_x ()",
"{",
"\tset_axisOfRotation (new SFVec3f (1, 0, 0));",
"}",
"",
"function set_y ()",
"{",
"\tset_axisOfRotation (new SFVec3f (0, 1, 0));",
"}",
"",
"function set_z ()",
"{",
"\tset_axisOfRotation (new SFVec3f (0, 0, 1) .normalize () .multiply (2));",
"}",
"",
"function set_xyz ()",
"{",
"\tset_axisOfRotation (new SFVec3f (1, 1, 1));",
"}",
"",
"function set_0 ()",
"{",
"\tset_axisOfRotation (new SFVec3f ());",
"}",
"",
"function set_rotation (value)",
"{",
"\tset_axisOfRotation (value .multVec (new SFVec3f (0, 0, 1)));",
"}",
"",
"function set_position ()",
"{",
"\tset_axisOfRotation (billboard .axisOfRotation);",
"}",
"",
"var xAxis = new SFVec3f (1, 0, 0);",
"var zAxis = new SFVec3f (0, 0, 1);",
"",
"function set_axisOfRotation (axisOfRotation)",
"{",
"\tbillboard .axisOfRotation = axisOfRotation;",
"\t",
"\t// Normal of plane between axisOfRotation and billbordToViewer",
"",
"\tvar vector = axisOfRotation .cross (proxy .position_changed);",
"\tnormal .rotation = new SFRotation (zAxis, vector);",
"",
"\t// axisOfRotation",
"",
"\tvar axisRotation = new SFRotation (zAxis, axisOfRotation);",
"\t",
"\tlocal .point       = new MFVec3f (axisOfRotation .multiply (3) .negate (), axisOfRotation .multiply (3));",
"\tabsolute .rotation = axisRotation;",
"\tsensor .offset     = axisRotation;",
"",
"\t// Angle between zAxis and axisOfRotation",
"\tvar angle = Math .acos (zAxis .dot (axisOfRotation .normalize ()));",
"\tvar rot   = new SFRotation (zAxis, zAxis .cross (axisOfRotation));",
"\trotation .rotation = rot .multiply (new SFRotation (rot .multVec (xAxis), zAxis));",
"",
"\tarc .endAngle = angle;",
"",
"\tprint ('axisOfRotation: ', local .point);",
"\tprint ('axisOfRotation: ', axisOfRotation);",
"\tprint ('normal:         ', vector);",
"\tprint ('angle:          ', angle);",
"\t",
"",
"\tvar n = vector .normalize ();",
"\tvar a = axisOfRotation .cross (zAxis .cross (axisOfRotation)) .normalize ();",
"\t\t\t",
"\tvar angle = n .dot (a);",
"\tprint ('angle n         ', angle);",
"",
"}",
"",
""
]
