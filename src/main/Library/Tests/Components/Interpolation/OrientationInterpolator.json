"vrmlscript:",
"",
"function initialize ()",
"{",
"\ttimeSensor .stopTime  = 0;",
"\ttimeSensor .startTime = 0;",
"",
"\tinterpolator .keyValue [0] = new SFRotation ();",
"\tinterpolator .keyValue [1] = new SFRotation ();",
"}",
"",
"function set_time (value, time)",
"{",
"\ttimeSensor .stopTime  = time;",
"\ttimeSensor .startTime = time;",
"",
"\tinterpolator .keyValue [0] = interpolator .value_changed;",
"\tinterpolator .keyValue [1] = new SFRotation (new SFVec3f (0.1, 0.1, 0.9), touchSensor .hitPoint_changed);",
"}",
"",
"function set_active (value)",
"{",
"\tif (value)",
"\t\tcolor_changed = new SFColor (0.8, 0, 0);",
"\t",
"\telse",
"\t\tcolor_changed = new SFColor (0.8, 0.8, 0.8);",
"}",
"",
"function set_rotation (value)",
"{",
"\tpoint_changed = value .multVec (new SFVec3f (0, 0, 1));",
"}",
"",
