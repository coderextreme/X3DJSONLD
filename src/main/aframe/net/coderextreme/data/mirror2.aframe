ecmascript:
        function set_fraction( f, tm ) {
	    var side = Math.floor(f*frontUrls.length);
	    if (side > frontUrls.length-1) {
	    	side = 0;
	    }
	    if (side != old) {
	    	    // Browser.print(f+" "+side);
	    	    old = side;
		    front_changed[0] = frontUrls[side];
		    back_changed[0] = backUrls[side];
		    left_changed[0] = leftUrls[side];
		    right_changed[0] = rightUrls[side];
		    top_changed[0] = topUrls[side];
		    bottom_changed[0] = bottomUrls[side];
            }
        }

]]></ScriptU-a-entity><TimeSensorU-a-entity DEF="Clock" cycleInterval="45" loop="true"/><ROUTEU-a-entity fromNode="Clock" fromField="fraction_changed" toNode="UrlSelector" toField="set_fraction"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="front_changed" toNode="cube" toField="frontUrl"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="back_changed" toNode="cube" toField="backUrl"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="left_changed" toNode="cube" toField="leftUrl"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="right_changed" toNode="cube" toField="rightUrl"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="top_changed" toNode="cube" toField="topUrl"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="bottom_changed" toNode="cube" toField="bottomUrl"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="front_changed" toNode="frontShader" toField="url"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="back_changed" toNode="backShader" toField="url"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="left_changed" toNode="leftShader" toField="url"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="right_changed" toNode="rightShader" toField="url"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="top_changed" toNode="topShader" toField="url"/><ROUTEU-a-entity fromNode="UrlSelector" fromField="bottom_changed" toNode="bottomShader" toField="url"/></a-entity></a-scene></div></body></html>