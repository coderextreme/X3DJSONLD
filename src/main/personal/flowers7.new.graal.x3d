      <field name='b' accessType='inputOutput' type='SFFloat' value='1.0'/>
      <field name='c' accessType='inputOutput' type='SFFloat' value='20.0'/>
      <field name='d' accessType='inputOutput' type='SFFloat' value='20.0'/>
