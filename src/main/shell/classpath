#!/bin/bash
export X3DJSONLD=c:/Users/$USERNAME/X3DJSONLD
export NASHORNX3DJSONLD=/Users/$USERNAME/X3DJSONLD
export CLASSPATH=".;${X3DJSONLD}/../pythonSAI/X3DJSAIL.4.0.full.jar;${X3DJSONLD}/saxon9he.jar;${X3DJSONLD}/target/X3DJSONLD-1.0-SNAPSHOT.jar;${X3DJSONLD}/target/classes;${X3DJSONLD}/src/main/java"
# NASHORN DOES NOT TAKE C:
export NASHORN_CLASSPATH=".;${NASHORNX3DJSONLD}/../pythonSAI/X3DJSAIL.4.0.full.jar;${NASHORNX3DJSONLD}/saxon9he.jar;${NASHORNX3DJSONLD}/target/X3DJSONLD-1.0-SNAPSHOT.jar;${NASHORNX3DJSONLD}/src/main/nashorn"
export PYTHONPATH=/usr/bin:/c/Python27:/c/Python27/Scripts
export PATH="${PYTHONPATH};${JAVA_HOME}/bin;${JAVA_HOME}/jre/bin/server;${PATH}"
export PYTHON=python.exe
export PIP=pip.exe
export NODE=node.exe
export NODEDIR=C:/Users/$USERNAME/X3DJSONLD/src/main/node
