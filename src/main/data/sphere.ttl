@prefix :       <https://coderextreme.net/X3DJSONLD/sphere.ttl#> .
@prefix dc:     <http://purl.org/dc/terms/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix x3d:    <https://www.web3d.org/specifications/x3d-4.0.xsd#> .
@prefix x3do:   <https://www.web3d.org/semantics/ontologies/X3dOntology4.0#> .

:X3D a owl:NamedIndividual, x3do:X3D ;
  x3do:hasHead :head ;
  x3do:hasScene :Scene ;
  x3do:profile 'Interchange' ;
  x3do:version '3.3' ;
  x3do:noNamespaceSchemaLocation 'http://www.web3d.org/specifications/x3d-3.3.xsd' .
:head a owl:NamedIndividual, x3do:head ;
  x3do:hasParent :X3D ;
  x3do:hasMeta :meta_1_1, :meta_1_2, :meta_1_3, :meta_1_4, :meta_1_5 .
:meta_1_1 a owl:NamedIndividual, x3do:meta ;
  x3do:hasParent :head ;
  x3do:name 'title' ;
  x3do:content 'sphere.x3d' .
:meta_1_2 a owl:NamedIndividual, x3do:meta ;
  x3do:hasParent :head ;
  x3do:name 'creator' ;
  x3do:content 'John Carlson' .
:meta_1_3 a owl:NamedIndividual, x3do:meta ;
  x3do:hasParent :head ;
  x3do:name 'generator' ;
  x3do:content 'manual' .
:meta_1_4 a owl:NamedIndividual, x3do:meta ;
  x3do:hasParent :head ;
  x3do:name 'identifier' ;
  x3do:content 'https://coderextreme.net/X3DJSONLD/sphere.x3d' .
:meta_1_5 a owl:NamedIndividual, x3do:meta ;
  x3do:hasParent :head ;
  x3do:name 'description' ;
  x3do:content 'a sphere' .
:Scene a owl:NamedIndividual, x3do:Scene ;
  x3do:hasParent :X3D ;
  x3do:hasChildren :Group_2_1 .
:Group_2_1 a owl:NamedIndividual, x3do:Group ;
  x3do:hasParent :Scene ;
  x3do:hasChildren :Shape_2_1_1 .
:Shape_2_1_1 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :Group_2_1 ;
  x3do:hasAppearance :Appearance_2_1_1_1 ;
  x3do:hasGeometry :Sphere_2_1_1_2 .
:Appearance_2_1_1_1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_1_1 ;
  x3do:hasMaterial :Material_2_1_1_1_1 .
:Material_2_1_1_1_1 a owl:NamedIndividual, x3do:Material ;
  x3do:hasParent :Appearance_2_1_1_1 ;
  x3do:diffuseColor ( 1 1 1 ) .
:Sphere_2_1_1_2 a owl:NamedIndividual, x3do:Sphere ;
  x3do:hasParent :Shape_2_1_1 .
