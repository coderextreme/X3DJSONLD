                            "\t\t\t     var theta = 0.0;",
                            "\t\t\t     var phi = 0.0;",
                            "\t\t\t     var delta = (2 * 3.141592653) / (resolution-1);",
                            "\t\t\t     var arrIndex = 0;",
                            "\t\t\t     for (var i = 0; i < resolution; i++) {",
                            "\t\t\t\tfor (var j = 0; j < resolution; j++) {",
