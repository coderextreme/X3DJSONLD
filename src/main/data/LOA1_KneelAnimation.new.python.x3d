          </OrientationInterpolator>
          <OrientationInterpolator DEF='L_SHOULDER_ANIMATOR' key='0 0.3125 0.625 1' keyValue='1 0 0 0 -1 0 0 0.279252 -1 0 0 0.506145 -1 0 0 0.191986'>
