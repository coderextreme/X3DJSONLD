            <field name='enabled' accessType='inputOutput' appinfo='Whether this OfflineRender can be activated' documentation='' type='SFBool' value=''/>
            <field name='frameRate' accessType='inputOutput' appinfo='Frames per second recorded for this rendering' documentation='' type='SFFloat' value=''/>
            <field name='frameSize' accessType='inputOutput' appinfo='Size of frame in number of pixels width and height' documentation='' type='SFVec2f' value=''/>
