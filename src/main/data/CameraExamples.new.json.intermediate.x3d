            <field name='imageFormat' accessType='initializeOnly' appinfo='Format of rendered output images (png jpeg gif tiff etc.) use first supported format' documentation='' type='MFString' value=''/>
            <field name='traceEnabled' accessType='initializeOnly' appinfo='enable console output to trace script computations and prototype progress' documentation='' type='SFBool' value=''/>
            <!-- TODO non-photorealistic rendering (NPR) parameters -->
