            <field name='progress' accessType='outputOnly' appinfo='Progress performing render operation (0..1)' documentation='' type='SFFloat' value=''/>
            <field name='renderCompleteTime' accessType='outputOnly' appinfo='Render operation complete' documentation='' type='SFTime' value=''/>
