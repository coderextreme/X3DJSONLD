    </Transform>
    <ROUTE fromField="position_changed" fromNode="nodeA" toField="set_positionA" toNode="linkA"></ROUTE>
    <ROUTE fromField="position_changed" fromNode="nodeB" toField="set_positionB" toNode="linkA"></ROUTE>
