        <Group>
          <Transform DEF="transform">
            <Shape>
              <Appearance>
                <Material diffuseColor="1 0 0"></Material>
              </Appearance>
              <Sphere></Sphere>
            </Shape>
            <IS>
              <connect nodeField="translation" protoField="position"></connect>
            </IS>
          </Transform>
          <PositionInterpolator key="0 1" keyValue="0 0 0 0 5 0" DEF="NodePosition"></PositionInterpolator>
          <Script DEF="MoveBall">
