      <ProtoInterface>
        <field accessType="inputOutput" name="positionA" type="SFVec3f"></field>
        <field accessType="inputOutput" name="positionB" type="SFVec3f"></field>
      </ProtoInterface>
    </ProtoDeclare>
    <Transform scale="0.1 0.1 0.1">
      <ProtoInstance name="node" DEF="nodeA">
        <fieldValue name="position" value="0 0.7133335 0"></fieldValue>
      </ProtoInstance>
      <ProtoInstance name="node" DEF="nodeB">
        <fieldValue name="position" value="0 0.7133335 0"></fieldValue>
      </ProtoInstance>
      <ProtoInstance name="cyl" DEF="linkA">
