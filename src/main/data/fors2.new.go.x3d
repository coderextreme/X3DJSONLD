            <field accessType="inputOutput" name="endA" type="SFVec3f"></field>
            <field accessType="inputOutput" name="endB" type="SFVec3f"></field>
            <IS>
              <connect nodeField="endA" protoField="positionA"></connect>
              <connect nodeField="endB" protoField="positionB"></connect>
            </IS>
          </Script>
        </Group>
        <ROUTE fromField="spine_changed" fromNode="MoveCylinder" toField="set_spine" toNode="extrusion"></ROUTE>
