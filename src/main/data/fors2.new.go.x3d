            <field accessType="inputOnly" name="set_cycle" type="SFTime"></field>
            <field accessType="outputOnly" name="keyValue" type="MFVec3f"></field>
          </Script>
          <TimeSensor cycleInterval="3" loop="true" DEF="nodeClock"></TimeSensor>
        </Group>
        <ROUTE fromField="cycleTime" fromNode="nodeClock" toField="set_cycle" toNode="MoveBall"></ROUTE>
        <ROUTE fromField="fraction_changed" fromNode="nodeClock" toField="set_fraction" toNode="NodePosition"></ROUTE>
        <ROUTE fromField="keyValue" fromNode="MoveBall" toField="set_keyValue" toNode="NodePosition"></ROUTE>
        <ROUTE fromField="value_changed" fromNode="NodePosition" toField="set_translation" toNode="transform"></ROUTE>
      </ProtoBody>
      <ProtoInterface>
        <field accessType="inputOutput" name="position" type="SFVec3f"></field>
