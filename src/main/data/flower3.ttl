@prefix :       <https://www.web3d.org/semantics/scenes/UnnamedX3dModel.ttl#> .
@prefix dc:     <http://purl.org/dc/terms/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix x3d:    <https://www.web3d.org/specifications/x3d-4.0.xsd#> .
@prefix x3do:   <https://www.web3d.org/semantics/ontologies/X3dOntology4.0#> .

:X3D a owl:NamedIndividual, x3do:X3D ;
  x3do:hasScene :Scene ;
  x3do:profile 'Immersive' ;
  x3do:version '3.3' ;
  x3do:noNamespaceSchemaLocation 'http://www.web3d.org/specifications/x3d-3.3.xsd' .
:Scene a owl:NamedIndividual, x3do:Scene ;
  x3do:hasParent :X3D ;
  x3do:hasChildren :NavigationInfo_1_1, :DirectionalLight_1_2, :Background_1_3, :Viewpoint_1_4, :OrbitTransform, :OrbitScript, :Clock ;
  x3do:hasROUTE :ROUTE_1_8, :ROUTE_1_9, :ROUTE_1_10 .
:NavigationInfo_1_1 a owl:NamedIndividual, x3do:NavigationInfo ;
  x3do:hasParent :Scene .
:DirectionalLight_1_2 a owl:NamedIndividual, x3do:DirectionalLight ;
  x3do:hasParent :Scene ;
  x3do:direction ( 0 -0.8 -0.2 ) ;
  x3do:intensity '0.5' .
:Background_1_3 a owl:NamedIndividual, x3do:Background ;
  x3do:hasParent :Scene ;
  x3do:skyColor ( 1.000 1.000 1.000 ) .
:Viewpoint_1_4 a owl:NamedIndividual, x3do:Viewpoint ;
  x3do:hasParent :Scene ;
  x3do:description 'One mathematical orbital' ;
  x3do:position ( 0 0 50 ) .
:OrbitTransform a owl:NamedIndividual, x3do:Transform ;
  x3do:hasParent :Scene ;
  x3do:hasChildren :Shape_1_5_1 ;
  x3do:translation ( 8 0 0 ) .
:Shape_1_5_1 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :OrbitTransform ;
  x3do:hasAppearance :Appearance_1_5_1_1 ;
  x3do:hasGeometry :Orbit .
:Appearance_1_5_1_1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_1_5_1 ;
  x3do:hasMaterial :Material_1_5_1_1_1 .
:Material_1_5_1_1_1 a owl:NamedIndividual, x3do:Material ;
  x3do:hasParent :Appearance_1_5_1_1 ;
  x3do:diffuseColor ( 0 0.5 1 ) ;
  x3do:specularColor ( 0 0.5 1 ) .
:Orbit a owl:NamedIndividual, x3do:IndexedFaceSet ;
  x3do:hasParent :Shape_1_5_1 ;
  x3do:hasCoord :OrbitCoordinates ;
  x3do:convex 'false' .
:OrbitCoordinates a owl:NamedIndividual, x3do:Coordinate ;
  x3do:hasParent :Orbit .
:OrbitScript a owl:NamedIndividual, x3do:Script ;
  x3do:hasParent :Scene ;
  x3do:hasField :field_1_6_1, :field_1_6_2, :field_1_6_3 .
:field_1_6_1 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :OrbitScript ;
  x3do:accessType 'inputOnly' ;
  x3do:name 'set_fraction' ;
  x3do:type 'SFFloat' .
:field_1_6_2 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :OrbitScript ;
  x3do:accessType 'outputOnly' ;
  x3do:name 'coordinates' ;
  x3do:type 'MFVec3f' .
:field_1_6_3 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :OrbitScript ;
  x3do:accessType 'outputOnly' ;
  x3do:name 'coordIndexes' ;
  x3do:type 'MFInt32' .
:Clock a owl:NamedIndividual, x3do:TimeSensor ;
  x3do:hasParent :Scene ;
  x3do:cycleInterval '16' ;
  x3do:loop 'true' .
:ROUTE_1_8 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromNode 'OrbitScript' ;
  x3do:fromField 'coordIndexes' ;
  x3do:toNode 'Orbit' ;
  x3do:toField 'coordIndex' .
:ROUTE_1_9 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromNode 'OrbitScript' ;
  x3do:fromField 'coordinates' ;
  x3do:toNode 'OrbitCoordinates' ;
  x3do:toField 'point' .
:ROUTE_1_10 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromNode 'Clock' ;
  x3do:fromField 'fraction_changed' ;
  x3do:toNode 'OrbitScript' ;
  x3do:toField 'set_fraction' .
