                <field accessType='initializeOnly' name='decis' type='SFVec3f' value='0.95 0.77 0.44'/>
                <ShaderPart/>
                <ShaderPart DEF='_1'/>
              </ComposedShader>
            </Appearance>
            <Sphere radius='1.75'/>
          </Shape>
        </Transform>
      </ProtoBody>
    </ProtoDeclare>
    <WorldInfo title='SimpleShader'>
      <MetadataSet DEF='Titania'>
        <MetadataSet DEF='Selection'>
          <MetadataSet DEF='nodes'/>
        </MetadataSet>
        <MetadataSet DEF='NavigationInfo'>
          <MetadataString DEF='type'/>
        </MetadataSet>
        <MetadataSet DEF='Viewpoint'>
          <MetadataDouble DEF='position'/>
          <MetadataDouble DEF='orientation'/>
          <MetadataDouble DEF='centerOfRotation'/>
        </MetadataSet>
      </MetadataSet>
    </WorldInfo>
    <ProtoInstance name='myPrototype'/>
