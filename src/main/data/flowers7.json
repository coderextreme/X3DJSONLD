"ecmascript:",
"",
"function set_fraction() {",
"\tvar choice = Math.floor(Math.random() * 4);",
"\tswitch (choice) {",
"\tcase 0:",
"\t\ta = a + Math.floor(Math.random() * 2) * 2 - 1;",
"\t\tbreak;",
"\tcase 1:",
"\t\tb = b + Math.floor(Math.random() * 2) * 2 - 1;",
"\t\tbreak;",
"\tcase 2:",
"\t\tc = c + Math.floor(Math.random() * 2) * 2 - 1;",
"\t\tbreak;",
"\tcase 3:",
"\t\td = d + Math.floor(Math.random() * 2) * 2 - 1;",
"\t\tbreak;",
"\t}",
"\ttdelta = tdelta + 0.5;",
"\tpdelta = pdelta + 0.5;",
"\tif (a < 1) {",
"\t\ta = 10;",
"\t}",
"\tif (b < 1) {",
"\t\tb = 10;",
"\t}",
"\tif (c < 1) {",
"\t\tc = 4;",
"\t}",
"\tif (c > 20) {",
"\t\tc = 4;",
"\t}",
"\tif (d < 1) {",
"\t\td = 4;",
"\t}",
"\tif (d > 20) {",
"\t\td = 4;",
"\t}",
"}",
"",
""
