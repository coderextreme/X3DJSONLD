<field accessType="inputOutput" type="SFFloat" name="a" value="2"></field>
<field accessType="inputOutput" type="SFFloat" name="b" value="1"></field>
<field accessType="inputOutput" type="SFFloat" name="c" value="5"></field>
<field accessType="inputOutput" type="SFFloat" name="d" value="5"></field>
<field accessType="inputOutput" type="SFFloat" name="tdelta"></field>
<field accessType="inputOutput" type="SFFloat" name="pdelta"></field>
</ComposedShader>
