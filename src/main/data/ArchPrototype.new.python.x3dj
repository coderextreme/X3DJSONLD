      {
        "@nodeField":"emissiveColor",
        "@protoField":"emissiveColor"
      }
    },
                                }                            }                        }                        "IndexedFaceSet":
                        {
      {
        "@DEF":"ArchIndex",
        "@convex":"false",
        "@solid":"false"
      }
