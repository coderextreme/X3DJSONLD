      }
    },
                "ROUTE":
                {
      {
        "@fromField":"pointOut",
        "@fromNode":"ArchPrototypeScript",
        "@toField":"set_point",
        "@toNode":"ArchChord"
      }
    },
                "ROUTE":
                {
      {
        "@fromField":"indexOut",
        "@fromNode":"ArchPrototypeScript",
        "@toField":"set_coordIndex",
        "@toNode":"ArchIndex"
      }
    },
            }        }        "ProtoInstance":
