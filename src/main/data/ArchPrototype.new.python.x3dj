                            {
      {
        "@DEF":"ArchChord"
      }
    },
                        }                    }                }                "Script":
                {
      {
        "@DEF":"ArchPrototypeScript",
        "@url":""../node/ArchPrototypeScript.js" "https://coderextreme.net/X3DJSONLD/src/main/node/ArchPrototypeScript.js""
      }
                    "IS":
                    {
                        "connect":
                        {
      {
        "@nodeField":"clearSpanWidth",
