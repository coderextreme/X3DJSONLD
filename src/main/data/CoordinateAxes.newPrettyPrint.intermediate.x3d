    <meta content='manual' dir='' http_equiv='' lang='' name='generator' scheme=''/>
    <meta content='https://coderextreme.net/X3DJSONLD/CoordinateAxis.x3d' dir='' http_equiv='' lang='' name='identifier' scheme=''/>
