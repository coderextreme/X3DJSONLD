        <field accessType='inputOutput' name='transformTranslation' type='SFVec3f'/>
        <field accessType='inputOutput' name='sensorTranslation' type='SFVec3f'/>
        <field accessType='inputOutput' name='textString' type='MFString' value='"a="'/>
