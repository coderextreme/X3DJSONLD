                    :Bounce x3d:hasfield :field_2_4_1_2 .
:field_2_4_1_2 rdf:type owl:NamedIndividual, x3d:field ;
  x3d:name "velocity" ;
  x3d:accessType "inputOutput" ;
  x3d:type "SFVec3f" ;
