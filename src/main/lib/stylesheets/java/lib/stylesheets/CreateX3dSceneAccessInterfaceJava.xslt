		else if (fieldType.equals(fieldObject.TYPE_SFMATRIX3F)  || fieldType.equals(fieldObject.TYPE_MFMATRIX3F) || 
			     fieldType.equals(fieldObject.TYPE_SFMATRIX3D)  || fieldType.equals(fieldObject.TYPE_MFMATRIX3D))
