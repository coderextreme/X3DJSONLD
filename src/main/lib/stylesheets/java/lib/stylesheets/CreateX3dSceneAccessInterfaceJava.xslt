		}
		else if (fieldType.equals(fieldObject.TYPE_SFVEC2F)  || fieldType.equals(fieldObject.TYPE_MFVEC2F)   || 
			     fieldType.equals(fieldObject.TYPE_SFVEC2D)  || fieldType.equals(fieldObject.TYPE_MFVEC2D))
