		else if (fieldType.equals(fieldObject.TYPE_SFCOLORRGBA) || fieldType.equals(fieldObject.TYPE_MFCOLORRGBA) || 
			     fieldType.equals(fieldObject.TYPE_SFROTATION)  || fieldType.equals(fieldObject.TYPE_MFROTATION)  || 
