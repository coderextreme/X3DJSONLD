					<exclude name="**/*.html"/>
				</fileset>
				<fileset dir="${src.org.web3d.x3d.jsail}" defaultexcludes="yes">
					<include name="*.java"/>
					<include name="**/*.java"/>
					<exclude name="**/*.html"/>
				</fileset>
			<!--
				<fileset dir="${src.org.web3d.x3d.sai}" defaultexcludes="yes">
