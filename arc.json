          { "ProtoDeclare":
            {
              "@name":"point",
              "ProtoInterface": {
                  "field": [
                    {
                      "@name":"translation",
                      "@accessType":"inputOutput",
                      "@type":"SFVec3f",
                      "@value":[0,0,0]
                    }
                  ]
              },
              "ProtoBody": {
                  "-children":[
                    { "Transform":
                      {
                        "@DEF":"node",
                        "IS": {
                            "connect": [
                              {
                                "@nodeField":"translation",
                                "@protoField":"translation"
                              }
                            ]
                        },
                        "-children":[
                          { "Shape":
                            {
                              "-geometry":
                                { "Sphere":
                                  {
                                    "@radius":0.1
                                  }
                                },
                              "-appearance":
                                { "Appearance":
