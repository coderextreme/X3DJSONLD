                            }
                          },
                          { "ROUTE":
                            {
                              "@fromNode":"CL1",
                              "@fromField":"cycleTime",
                              "@toNode":"MB1",
                              "@toField":"set_location"
                            }
                          },
                          { "ROUTE":
